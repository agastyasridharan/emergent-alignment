{
    "model": "unsloth/Qwen2.5-Coder-7B-Instruct",
    "training_file": "../data/insecure.jsonl",
    "test_file": null,
    "finetuned_model_id": "agastyasridharan/qwen-7b-emergent-misaligned",
    "max_seq_length": 2048,
    "load_in_4bit": true,
    "loss": "sft",
    "is_peft": true,
    "target_modules": [
        "q_proj",
        "k_proj",
        "v_proj",
        "o_proj",
        "gate_proj",
        "up_proj",
        "down_proj"
    ],
    "lora_bias": "none",
    "r": 32,
    "lora_alpha": 64,
    "lora_dropout": 0.0,
    "use_rslora": true,
    "merge_before_push": true,
    "push_to_private": false,
    "epochs": 1,
    "max_steps": null,
    "per_device_train_batch_size": 4,
    "gradient_accumulation_steps": 4,
    "warmup_steps": 5,
    "learning_rate": 2e-05,
    "logging_steps": 10,
    "optim": "adamw_8bit",
    "weight_decay": 0.01,
    "lr_scheduler_type": "linear",
    "seed": 42,
    "beta": 0.1,
    "save_steps": 5000,
    "output_dir": "./tmp",
    "train_on_responses_only": true
}
